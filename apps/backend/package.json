    "@types/passport": "^1.0.16",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
